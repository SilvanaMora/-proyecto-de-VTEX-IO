    "props": {
      "blockClass": "global__header--menu"
    }
  },
  "menu-item#food-global": {
    "props": {
      "id": "menu-item-food",
      "type": "custom",
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "#",
        "noFollow": false,
        "tagTitle": "Food",
        "text": "Food"
      },
      "iconProps": {
        "id": "food",
        "size": 16,
        "viewBox": "0 0 21 21",
