{
  "flex-layout.row#global__header-menu-item": {
    "props": {
      "horizontalAlign": "center",
      "fullWidth": true,
      "verticalAlign": "center",
      /*"preventHorizontalStretch": true,
      "preventVerticalStretch": true,*/
      "blockClass": "global__header-menu-item"
    },
    "children": ["vtex.menu@2.x:menu#global__header--menu-item"]
  },
  "vtex.menu@2.x:menu#global__header--menu-item": {
    "title": "Global header menu item",
    "children": [
      "menu-item#food-global",
