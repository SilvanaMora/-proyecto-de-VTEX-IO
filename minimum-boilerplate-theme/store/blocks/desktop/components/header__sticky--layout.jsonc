    }
  },
  "flex-layout.row#desktop__header--3-ofertas-enero": {
    "children": [
      "rich-text#desktop__header--text-ofertas-enero"
    ],
    "props": {
      "blockClass":"desktop__header--col-bar"
    }
  },
  "flex-layout.row#desktop__header--4-cyber-monday": {
    "children": [
      "rich-text#desktop__header--text-cyber-monday"
