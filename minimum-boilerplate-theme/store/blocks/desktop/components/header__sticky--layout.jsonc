{
  "sticky-layout#desktop__header": {
    "props": {
      "blockClass": "desktop__header",
      "fullWidth": true
    },
    "children": [
      "flex-layout.row#desktop__header--main"
    ]
  },
  "flex-layout.row#desktop__header--main": {
    "props": {
      "horizontalAlign": "between",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "blockClass": "desktop__header--main"

    },
    "children": [
     "flex-layout.col#desktop__mega-menu",
     "flex-layout.col#desktop__logo",
      "search-bar",
      "flex-layout.col#desktop__header--bar", 
      "minicart.v2",
      "login#global-login"
    ]
  },
  "flex-layout.col#desktop__logo": {
    "props": {
      "verticalAlign": "middle",
      "blockClass":"desktop__logo"
},
    "children": ["logo#global__logo-itg"]
  },

"flex-layout.col#desktop__header--bar":{
  "props": {
    "blockClass": "desktop__header--bar"
  },
  "children": [   
    "flex-layout.row#desktop__header--1-tendencias-it-globers",
    "flex-layout.row#desktop__header--2-coleccion-del-mes",
    "flex-layout.row#desktop__header--3-ofertas-enero",
    "flex-layout.row#desktop__header--4-cyber-monday",
    "flex-layout.row#desktop__header--5-never-say-never"]
},
  "flex-layout.row#desktop__header--1-tendencias-it-globers": {
    "children": [
      "rich-text#desktop__header--text-tendencias-it-globers"
    ],
