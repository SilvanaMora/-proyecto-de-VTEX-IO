{
  "sticky-layout#desktop__header": {
    "props": {
      "blockClass": "desktop__header"
    },
    "children": [
      "flex-layout.row#desktop__header--main",
      "flex-layout.row#desktop__header-bar-options2"
    ]
  },
  "flex-layout.row#desktop__header--main": {
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true,
      "padding":"0 20px",
      "blockClass": "desktop__header--main"
    },
    "children": [
      "flex-layout.col#spacer",
      "flex-layout.col#desktop__menu-icon",
      "flex-layout.col#desktop__logo",
      "flex-layout.col#spacer",
      "search-bar",
      "flex-layout.col#spacer",
      "flex-layout.col#desktop__header--bar-options-col", 
      "flex-layout.col#spacer",
     // "locale-switcher",
      "minicart.v2",
      "login",
      "flex-layout.col#spacer" 
      
    ]
  },
  "flex-layout.col#desktop__logo": {
    "props": {
      "verticalAlign": "middle",
      "padding": "10px 0",
      "blockClass":"flex-layout.col#desktop__logo"
},
    "children": ["image#global__logo-itg"]
  },
  "flex-layout.col#desktop__menu-icon": {
    "props": {
      "verticalAlign": "middle",
      "padding": "10px 0",
      "blockClass":"flex-layout.col#desktop__menu-icon"
},
    "children": ["image#global__menu-icon"]
},
"flex-layout.col#desktop__header--bar-options-col":{
  "props": {
    "blockClass": "desktop__header--bar-options-col"
  },
  "children": ["flex-layout.row#desktop__header--bar-options-fila"]
},
