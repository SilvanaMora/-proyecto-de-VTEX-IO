    ],
    "props": {
      "width": "8.33%"
    }
  },
  "flex-layout.col#col-5-hotel": {
    "children": [
      "image#col-5-image-hotel",
      "rich-text#col-5-text-hotel"
    ],
    "props": {
      "width": "8.33%"
    }
  },
  "image#col-1-image-food": {
    "props": {
      "src": "/arquivos/food.png",
      "alt": "Food"
    }
  },
  "rich-text#col-1-text-food": {
